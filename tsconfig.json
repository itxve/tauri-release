{
  "compilerOptions": {
    "target": "es2017",
    "module": "ESNext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
